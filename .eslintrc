{
    "extends": ["airbnb","plugin:react/recommended"],
    
    "parser":"babel-eslint",
    
    "parserOptions": {
        "ecmaVersion": 6
    },

    "plugins": [
        "react"
    ],
    
    "env": {
        "node": true,
        "browser": true,
        "es6": true
    },
    "rules": {
        "comma-dangle":"warn",
        "indent":["error", 4],
        "no-unused-vars":[
            "error",
            {
                "vars":"local",
                "args":"none"
            }
        ]
        }
}